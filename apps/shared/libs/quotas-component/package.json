{
    "name": "gts-quotas-component",
    "private": false,
    "version": "1.2.9",
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        "./component": {
            "import": "./dist/QuotasComponent.js",
            "require": "./dist/QuotasComponent.cjs"
        },
        "./lib": {
            "import": "./dist/lib.js",
            "require": "./dist/lib.cjs"
        },
        "./types": {
            "import": "./dist/components/lib/types.d.ts",
            "require": "./dist/components/lib/types.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "component": [
                "dist/QuotasComponent.d.ts"
            ],
            "lib": [
                "dist/components/lib/index.d.ts"
            ],
            "types": [
                "dist/components/lib/types.d.ts"
            ]
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "npm run lint && vite build",
        "lint:scripts": "eslint --ext .js,.ts,.vue ./src",
        "lint:scripts:fix": "npm run lint:scripts -- --fix",
        "lint:styles": "stylelint \"./src/**/*.{scss,vue}\"",
        "lint:styles:fix": "npm run lint:styles -- --fix",
        "lint:types": "tsc --noEmit",
        "lint:types:vue": "vue-tsc --noEmit",
        "lint": "npm run lint:scripts; npm run lint:styles; npm run lint:types; npm run lint:types:vue"
    },
    "volta": {
        "node": "18.16.0"
    },
    "peerDependencies": {
        "@ttskch/select2-bootstrap4-theme": "^1.5.2",
        "@floating-ui/vue": "^1.0.0",
        "@vueuse/components": "^10.0.2",
        "@vueuse/core": "^10.0.2",
        "bootstrap": "^5.3.2",
        "floating-vue": "^2.0.0-beta.20",
        "gts-common": "^1.2.3",
        "gts-components": "^1.2.3",
        "jquery": "^3.6.0",
        "luxon": "^3.3.0",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.2",
        "pretty-bytes": "^6.1.0",
        "qs": "^6.11.1",
        "sass": "^1.62.0",
        "select2": "^4.1.0-rc.0",
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@types/bootstrap": "^5.2.6",
        "@types/dotenv-safe": "^8.1.2",
        "@types/lodash": "^4.14.194",
        "@types/luxon": "^3.3.0",
        "@types/postcss-preset-env": "^8.0.0",
        "@types/jquery": "^3.5.16",
        "@types/qs": "^6.9.7",
        "@types/select2": "^4.0.62",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
        "dotenv-expand": "^10.0.0",
        "dotenv-safe": "^8.2.0",
        "eslint": "^8.38.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-define-config": "^1.18.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-plugin-vue": "^9.11.0",
        "laravel-vite-plugin": "^0.7.4",
        "lefthook": "^1.3.10",
        "postcss-html": "^1.5.0",
        "postcss-preset-env": "^8.3.2",
        "stylelint": "^15.5.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-hudochenkov": "^9.0.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard-scss": "^8.0.0",
        "stylelint-order": "^6.0.3",
        "typescript": "^4.9.5",
        "vite": "^4.3.1",
        "vite-plugin-checker": "^0.5.6",
        "vite-tsconfig-paths": "^4.2.0",
