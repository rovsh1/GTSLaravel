{
    "name": "gts-common",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        "./date": {
            "import": "./dist/date.js",
            "require": "./dist/date.cjs"
        },
        "./timezone": {
            "import": "./dist/timezone.js",
            "require": "./dist/timezone.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "date": [
                "dist/helpers/date.d.ts"
            ],
            "timezone": [
                "dist/support/timezone.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "npm run lint && vite build",
        "lint:scripts": "eslint --ext .js,.ts ./src",
        "lint:scripts:fix": "npm run lint:scripts -- --fix",
        "lint:styles": "stylelint \"./src/**/*.{scss}\"",
        "lint:styles:fix": "npm run lint:styles -- --fix",
        "lint:types": "tsc --noEmit",
        "lint": "npm run lint:scripts; npm run lint:styles; npm run lint:types"
    },
    "volta": {
        "node": "18.16.0"
    },
    "devDependencies": {
        "@ttskch/select2-bootstrap4-theme": "^1.5.2",
        "@types/bootstrap": "^5.2.6",
        "@types/cleave.js": "^1.4.12",
        "@types/jquery": "^3.5.16",
        "@types/lodash": "^4.14.194",
        "@types/luxon": "^3.3.0",
        "@types/node": "^20.11.19",
        "@types/postcss-preset-env": "^8.0.0",
        "@types/select2": "^4.0.62",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
        "bootstrap": "^5.3.2",
        "cleave.js": "^1.6.0",
        "eslint": "^8.38.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-define-config": "^1.18.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-plugin-vue": "^9.11.0",
        "jquery": "^3.6.0",
        "jquery-ui": "^1.13.0",
        "lefthook": "^1.3.10",
        "litepicker": "^2.0.12",
        "lodash": "^4.17.21",
        "luxon": "^3.3.0",
        "postcss-html": "^1.5.0",
        "postcss-preset-env": "^8.3.2",
        "pretty-bytes": "^6.1.0",
        "sass": "^1.62.0",
        "select2": "^4.1.0-rc.0",
        "stylelint": "^15.5.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-hudochenkov": "^9.0.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard-scss": "^8.0.0",
        "stylelint-order": "^6.0.3",
        "typescript": "^4.9.5",
        "vite": "^4.3.1",
        "vite-tsconfig-paths": "^4.2.0",
        "vue-tsc": "^1.2.0",
        "zod": "^3.21.4",
        "vite-plugin-checker": "^0.6.4",
        "vite-plugin-dts": "^3.7.2",
        "vite-plugin-css-injected-by-js": "^3.4.0"
    }
}